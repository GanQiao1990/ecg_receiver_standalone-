# ECG Receiver Configuration
# Copy this file to .env and fill in your API credentials

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_URL=https://api.gptnb.ai/

# Optional: Default patient information
DEFAULT_PATIENT_AGE=
DEFAULT_PATIENT_GENDER=
DEFAULT_SYMPTOMS=

# Diagnosis Settings
AUTO_DIAGNOSIS_INTERVAL=30  # seconds
DIAGNOSIS_BUFFER_SIZE=5000  # ECG samples to keep for diagnosis